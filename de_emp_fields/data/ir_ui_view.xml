<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="studio_customization.odoo_studio_hr_emplo_76afe9a6-a88e-429a-a707-5786fa6ba7b7" model="ir.ui.view" context="{'studio': True}">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='address_home_id']" position="before">
          <field name="x_studio_so_wo_do" string="S/O - W/O - D/O"/>
        </xpath>
        <xpath expr="//field[@name='km_home_work']" position="after">
          <field name="x_studio_disable" string="Disable"/>
          <field name="x_studio_religion" string="Religion"/>
          <field name="x_studio_referee_person" string="Referee Person"/>
          <field name="x_studio_equipment_acquired" string="Equipment Acquired"/>
        </xpath>
        <xpath expr="//field[@name='birthday']" position="after">
          <field name="x_studio_age" string="Age"/>
        </xpath>
      </data>
    </field>
    <field name="field_parent" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="inherit_id" ref="hr.view_employee_form"/>
    <field name="key" eval="False"/>
    <field name="mode">extension</field>
    <field name="model">hr.employee</field>
    <field name="name">Odoo Studio: hr.employee.form customization</field>
    <field name="priority">99</field>
    <field name="type">form</field>
  </record>
</odoo>
