<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>

        <record id="custom_proforma_format" model="report.paperformat">
            <field name="name">Jounal Voucher Paper Format</field>
            <field name="format">custom</field>
            <field name="page_height">270</field>
            <field name="page_width">250</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">40</field>
            <field name="margin_bottom">23</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_spacing">35</field>
            <field name="dpi">90</field>
        </record>

        <report
                id="report_custom_proforma1_pdf"
                string="Custom Proforma"
                model="sale.order"
                report_type="qweb-pdf"
                file="de_custom_proforma_invoice.custom_proforma_invoice1"
                name="de_custom_proforma_invoice.custom_proforma_invoice1"
                menu = "False"
        />

        <report
                id="cycle_gear_commercial_invoice_action"
                string="Commercial Proforma"
                model="sale.order"
                report_type="qweb-pdf"
                file="de_custom_proforma_invoice.cycle_gear_commercial"
                name="de_custom_proforma_invoice.cycle_gear_commercial"
                menu = "False"
        />
    </data>
</odoo>